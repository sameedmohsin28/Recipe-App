<h1>Food#index</h1>
<p>Find me in app/views/food/index.html.erb</p>
<ul>
<% @foods.each do |food| %>
    <li>
       <p><%= food.name %></p>
       <p><%= food.measurement_unit %> gram</p>
        <p><%= food.price %></p>
        <%= button_to "Delete", food_path(food), method: :delete %>
    </li>
<% end %>
<%= link_to 'Add Food', new_food_path %>
</ul>
